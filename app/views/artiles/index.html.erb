<h1>Artiles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @artiles.each do |artile| %>
      <tr>
        <td><%= artile.title %></td>
        <td><%= link_to 'Show', artile %></td>
        <td><%= link_to 'Edit', edit_artile_path(artile) %></td>
        <td><%= link_to 'Destroy', artile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

